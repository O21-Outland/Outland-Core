<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef Abstract="True" ParentName="OutlandFurnitureBase" Name="OutlandRugBase">
		<description>A simple rug for keeping your toesies warm.</description>
		<graphicData>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
		</graphicData>
		<comps>
			<li>
				<compClass>CompColorable</compClass>
			</li>
		</comps>
		<rotatable>true</rotatable>
		<altitudeLayer>Floor</altitudeLayer>
		<passability>Standable</passability>
		<pathCost>0</pathCost>
		<drawerType>MapMeshOnly</drawerType>
		<building>
			<isInert>true</isInert>
			<isEdifice>false</isEdifice>
		</building>
		<castEdgeShadows>false</castEdgeShadows>
		<fillPercent>0</fillPercent>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<WorkToBuild>5</WorkToBuild>
			<Mass>0.1</Mass>
			<Flammability>0</Flammability>
			<Beauty>1</Beauty>
		</statBases>
		<size>(1,1)</size>
		<costList>
			<Cloth>10</Cloth>
		</costList>
		<designationCategory>Outland_Outland</designationCategory>
		<uiIconScale>1</uiIconScale>
		<filthLeaving Inherit="False" />
		<clearBuildingArea>false</clearBuildingArea>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<modExtensions>
			<li Class="TabulaRasa.DefModExt_SubCategoryBuilding">
				<subCategory>Outland_SubCatRugs</subCategory>
				<showOnlyInCategory>true</showOnlyInCategory>
			</li>
		</modExtensions>
	</ThingDef>

</Defs>