<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="OrganicProductBase">
		<defName>Outland_RationPack</defName>
		<label>ration pack</label>
		<description>A prepared mix of food perfectly preserved, the process makes it taste bland, but not offensive. While it never rots, it can still deteriorate if left outside. Great for traveling.</description>
		<thingClass>ThingWithComps</thingClass>
		<thingCategories>
			<li>Foods</li>
		</thingCategories>
		<graphicData>
			<texPath>Outland/Items/RationPack</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<socialPropernessMatters>true</socialPropernessMatters>
		<statBases>
			<Flammability>0.6</Flammability>
			<WorkToMake>1200</WorkToMake>
			<DeteriorationRate>1</DeteriorationRate>
			<MarketValue>12</MarketValue>
			<Mass>0.15</Mass>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<ingestible>
			<foodType>Meal</foodType>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Meal_Eat</ingestSound>
			<optimalityOffsetHumanlikes>-5</optimalityOffsetHumanlikes>
			<optimalityOffsetFeedingAnimals>-10</optimalityOffsetFeedingAnimals>
		</ingestible>
		<comps>
			<li Class="CompProperties_Ingredients">
				<noIngredientsFoodKind>Meat</noIngredientsFoodKind>
			</li>
		</comps>
		<allowedArchonexusCount>200</allowedArchonexusCount>
	</ThingDef>

</Defs>