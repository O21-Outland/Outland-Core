<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!-- VE Fishing Treasures-->
	<Operation Class="TabulaRasa.PatchOperation_FindModByID">
		<mods>
			<li>VanillaExpanded.VCEFAddon</li>
		</mods>
		<match Class="PatchOperationReplace">
			<xpath>Defs/VCE_Fishing.FishDef[defName="VCEF_SpecialBoneItem"]/thingDef</xpath>
			<value>
				<thingDef>Outland_Bones</thingDef>
			</value>
		</match>
	</Operation>

</Patch>